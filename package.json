    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.2.2",
    "babelify": "10.0.0",
    "css-loader": "5.0.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
