{
  "name": "react-client-boilerplate",
  "version": "2.0.0",
  "private": true,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/RyanPWalker/react-client-boilerplate.git"
  },
  "keywords": [
    "Ryan Preston Walker",
    "Ryan Walker",
    "FED",
